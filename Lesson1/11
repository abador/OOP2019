using System;

namespace Lesson2
{
    class Program
    {
        static void Main(string[] args)
        {

            Console.WriteLine("1: ");
            string haslo1 = Console.ReadLine();
            Console.WriteLine("2: ");
            string haslo2 = Console.ReadLine();

            if (haslo1 == haslo2)
                Console.WriteLine("takie same");
            else
                Console.WriteLine("inne");

            Console.ReadKey();
        }
    }
}
