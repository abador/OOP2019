<Window x:Class="Projekt.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt"
        mc:Ignorable="d"
        Title="League of Legends Helper" Height="600" Width="500">
    <Grid>
        <Button Content="Champions" Click="MoveToChampions" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,-150,0,0" Width="150" Height="50"/>
        <Button Content="Information" Click="MoveToInformation" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,100,0,0" Width="150" Height="50"/>
        <Button Content="Credits" Click="MoveToCredits" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,350,0,0" Width="150" Height="50"/>

    </Grid>
</Window>
