<Window x:Class="Lekcja12b.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lekcja12b"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="Kalkulator" Margin="100,50,100,0" VerticalAlignment="Top" Height="30" Loaded="ComboBox_Loaded" SelectionChanged="ComboBox_SelectionChanged"/>
        <TextBox x:Name="pierwsza" HorizontalAlignment="Left" Height="30" Margin="100,210,0,0" Text="Pierwsza liczba" VerticalAlignment="Top" Width="200" TextWrapping="Wrap" TextAlignment="Center"/>
        <TextBox x:Name="druga" HorizontalAlignment="Right" Height="30" Margin="0,210,100,0" TextWrapping="Wrap" Text="Druga liczba" VerticalAlignment="Top" Width="200" TextAlignment="Center"/>
        <Button x:Name="licz" Content="Licz" HorizontalAlignment="Left" Margin="300,286,0,0" VerticalAlignment="Top" Width="192" Height="35" Click="Licz_Click"/>
    </Grid>
</Window>
